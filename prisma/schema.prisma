generator client {
  provider = "prisma-client-js"
}

// データソースの定義
datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

// データベースのモデル定義
model Contact {
  id       Int      @id @default(autoincrement())
  email    String
  message  String
}

model Contact2 {
  id       Int      @id @default(autoincrement())
  email    String
  message  String
}

model List {
  id       Int      @id @default(autoincrement())
  src    String
  title  String
  date   String
  text   String
}

model Calender {
  id       Int      @id @default(autoincrement())
  src    String
  title  String
  date   String
  detail   String
}